@model object

@{
    ViewBag.Title = "title";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="@Url.Content("~/Content/Org/Org.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/Scripts/Org/Org.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/Org/OrgUtils.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/Org/ViewOrg.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/Games/Point.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/Org/OrgDataParser.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/Org/OrgCharter.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/Org/BoundingRect.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/Org/EmpDetailer.js")" type="text/javascript"></script>

<canvas>
</canvas>
<div id="empDetail">
    <h1><span id="infoDataName"></span></h1>
    <div id="empInfo">
        <div class="infoLine">
            <label>Title:</label>
            <span id="infoDataTitle" class="infoData"></span>
        </div>
        <div class="infoLine">
            <label>Office:</label>
            <span id="infoDataOffice" class="infoData"></span>
        </div>
        <div class="infoLine">
            <label>Supervisor:</label>
            <span id="infoDataSupervisor" class="infoData"></span>
        </div>
    </div>
    <hr/>
    <section id="empReports">
        <div id="directReports">
            <h1>Direct Reports</h1>
            <ul id="directReportsList">
            </ul>
        </div>
        <div id="inDirectReports">
            <h1>Indirect Reports</h1>
            <ul id="inDirectReportsList">
            </ul>
        </div>
    </section>
    <input type="button" id="closeEmpDetailBtn" value="Close"></input>
</div>
