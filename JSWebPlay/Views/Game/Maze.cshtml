@model object

@{
    ViewBag.Title = "title";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="@Url.Content("~/Content/Games/Maze.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/Scripts/Games/Maze.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/Games/Stack.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/Games/MazeGame.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/Games/Point.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/Games/Door.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/Games/Wall.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/Games/Cell.js")" type="text/javascript"></script>

<h2>Maze</h2>
<div id="board">
    <div id="leftPanel">
        <input type="button" id="CtlDrawMaze" value="Draw Maze"/>
        <div>
        <label>Size:</label><input id="ctlSize" name="ctlSize" type="text" maxlength="10" value="10"/>
        </div>
        <input type="button" id="CtlSolveMaze" value="Solve"/>
        <input type="button" id="CtlErasePath" value="Erase"/>
    </div>
    <canvas id="maze"></canvas>
</div>
