@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>Klein Jeopardy 75</title>
    <link href="@Url.Content("~/Content/Games/Jeopardy.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Games/GameData.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Games/Jeopardy.js")" type="text/javascript"></script>
</head>
<body>
    <header>
        <img id="headshot1" class="headshot" src="@Url.Content("~/Content/Images/rpk-headshot-3.png")" alt="RPK"/>
        <section id="headers">
            <h1>Klein Jeopardy 75</h1>
        </section>
        <img id="headshot2" class="headshot" src="@Url.Content("~/Content/Images/rpk-headshot-3.png")" alt="RPK"/>
        <div class="clear">
        </div>
    </header>
    <section id="boardSection">
            <audio id="barbara01" src="@Url.Content("~/Content/Audio/barbara-01.mp3")" />
            <audio id="idiot01" src="@Url.Content("~/Content/Audio/idiot.mp3")" />
            <audio id="lostkeys" src="@Url.Content("~/Content/Audio/lostkeys.mp3")" />

        
        <div id="revealedQuestion">
            <div id="questionText">
                The question is...
            </div>
            <div id="revealedButtons">
                <input id="CtlRevealAnswerBtn" type="button" value="And the answer is..." />
            </div>
        </div>
        
        <div id="revealedAnswer">
            <div id="answerText">
                The answer is...
            </div>
            <div id="revealedAnswerButtons">
                <input id="CtlRevealedAnswerCloseBtn" type="button" value="OK" />
            </div>
        </div>

        <div id="bigRichard">
            <img id="bigRichardImg" src="@Url.Content("~/Content/Images/dicky face.png")" alt="RPK"/>
        </div>

        <table id="cardTable">
            <thead id="categoryHeader">
            </thead>
            <tbody>
            </tbody>
        </table>
    </section>
    <section id="controlBarSection">
        <input type="button" id="CBtnStartGameBtn" class="controlButton" value="Start"/>
        <input type="button" id="CBtnWrong" class="controlButton" value="Wrong"/>
        <input type="button" id="CBtnBarbara" class="controlButton" value="Barbara!"/>
    </section>
</body>
</html>
