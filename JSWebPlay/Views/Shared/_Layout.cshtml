<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Shared/site.js")" type="text/javascript"></script>
</head>
<body>
    <ul id="mainMenu">
        <li><a href="#">Computation</a>
            <ul>
                <li>@Html.ActionLink("Fib", "Fib", "Home")</li>
                <li>@Html.ActionLink("Factorial", "Factorial", "Home")</li>
                <li>@Html.ActionLink("Css Tests", "CssTests", "Home")</li>
            </ul>
        </li>
        <li><a href="#">Games</a>
            <ul>
                <li>@Html.ActionLink("Jeopardy", "Jeopardy", "Game")</li>
                <li>@Html.ActionLink("Maze", "Maze", "Game")</li>
                <li>@Html.ActionLink("Stefanie!", "StefGame", "Game")</li>
            </ul>
        </li>
        <li>@Html.ActionLink("Org Chart", "ViewOrg", "Org")</li>
        <li><a href="#">Sketches</a>
            <ul>
                <li>@Html.ActionLink("Spirographs", "Spirograph", "Sketch")</li>
            </ul>
        </li>
        <li><a href="#">Graphs</a>
            <ul>
                <li>@Html.ActionLink("2-D Graphics", "Graph2d", "Graph")</li>
            </ul>
        </li>
        <li><a href="#">Spikes</a>
            <ul>
                <li>@Html.ActionLink("Scaling", "Scale", "Org")</li>
            </ul>
        </li>
    </ul>
    <div class="clear">
        &nbsp;</div>
    @RenderBody()
</body>
</html>
